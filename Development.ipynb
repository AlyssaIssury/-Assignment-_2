conn = sqlite3.connect("enron.db")
cur = conn.cursor()
# Retrieving table names
sql = """
SELECT name
FROM sqlite_master
WHERE type = "table";
"""
cur.execute(sql)
tables = cur.fetchall()

# Retrieving table information
for table in tables:
    table_name = table[0]
    sql = f"PRAGMA table_info('{table_name}');"
    cur.execute(sql)
    table_info = cur.fetchall()
    print(f"Table: {table_name}")
    for column in table_info:
        print(column)
    print("\n")

